                    <!-- pagination phân trang -->
                    <div class="box-footer ">
                        <div class="box-tools pull-right">
                            <ul uib-pagination
                                total-items="TotalItems"
                                ng-model="currentPage"
                                items-per-page="numPerPage"
                                boundary-links="true"
