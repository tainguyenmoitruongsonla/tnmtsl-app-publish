@using WaterResource.Service;
@{
    ViewBag.Title = "Thông tin công trình khai thác, sử dụng nước mặt";
    UserRightService _service = new UserRightService();
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (_service.IsViewAction("/cong-trinh", User.Identity.Name, "VIEW"))
{
    <style>
        .leaflet-popup-content {
            overflow-y: auto;
        }
    </style>
    <link href="~/Assets/leaflet/leaflet.css" rel="stylesheet" />
    <script src="~/Assets/leaflet/leaflet.js"></script>
    <script src="~/Assets/leaflet/L.KML.js"></script>
    <!-- Load Esri Leaflet from CDN -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/esri-leaflet/2.0.4/esri-leaflet.js"></script>

    <!-- Load Esri Leaflet Vector from CDN -->
    <script src="https://unpkg.com/esri-leaflet-vector@3.1.3/dist/esri-leaflet-vector.js" crossorigin=""></script>
    <script src="~/Assets/leaflet/leaflet-bing-layer.js"></script>
    <script src="~/Assets/leaflet/Bing.js"></script>
    <div ng-controller="myCntrl as demo">
        <script src="~/Scripts/Construction/Controller.js"></script>
        @Html.Partial("ImportServiceJs")

        <!-- Main content -->
        <section class="content p-0" id="Surfacewater">
            <div class="row m-0">
                <div class="col-md-12 p-0" style="width: 100%; height: calc( 65vh - 50px )">
                    <select id="basemaps" class="form-select switch-basemap" aria-label="Default select example">
                        <option value="Imagery">Bản đồ vệ tinh</option>
                        <option value="Topographic">Bản đồ địa hình</option>
                        <option value="Streets">Bản đồ đường</option>
                        <option value="Gray">Bản đồ xám</option>
                    </select>
                    <ul class="annotate-layer cursor-pointer" id="annotate-layer">
                        <li class="m-0 d-flex">
                            <input type="checkbox" class="mr-2" id="cons-name" ng-click="toggleShow()">
                            <label for="cons-name">Hiển thị tên công trình</label>
                        </li>
                    </ul>
                    <div class="bg-map" id="Map" style="width: 100%; height: 100%"></div>
                </div>
                <div class="col-md-12 p-0">
                    <div class="col-md-12 p-0">
                        <div class="row m-0 justify-content-between align-items-center px-2 py-1">
                            <div class="col-lg-3 p-0 row m-0 align-items-center">
                                <div class="col-12 p-0">Tổng số công trình KTSDN mặt: <span class="font-weight-bold text-success" ng-bind-html="TotalConstruction"></span> </div>
                                <div class="col-12 p-0">Số công trình đã cấp phép: <span class="font-weight-bold text-success" ng-bind-html="TotalConstruction - TotalConsNoLicense"></span> </div>
                            </div>
                            <div class="col-lg-9 row m-0 align-items-center justify-content-end pt-3 px-0 pt-md-0">
                                @Html.Partial("_search")
                            </div>
                        </div>

                        <div class="table-responsive wrapper-sticky">
                            <table class="table no-margin table-hover table-bordered" id="table-show-license">
                                <thead class="custom-table-thead">
                                    <tr>
                                        <th rowspan="2" class="text-center align-middle sticky-col start-col">STT</th>
                                        <th rowspan="2" class="text-center align-middle text-nowrap sticky-col start-col">Tên công trình</th>
                                        <th rowspan="2" class="text-center align-middle text-nowrap">Địa điểm</th>
                                        <th colspan="2" class="text-center align-middle text-nowrap">Tọa độ đập chính </th>
                                        <th rowspan="2" class="text-center align-middle text-nowrap">Nguồn nước khai thác</th>
                                        <th rowspan="2" class="text-center align-middle text-nowrap">Phương thức khai thác </th>
                                        <th rowspan="2" class="text-center align-middle text-nowrap">Chế độ KT</th>
                                        <th rowspan="2" class="text-center align-middle text-nowrap">Mục đích KT</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" rowspan="2" class="text-center align-middle text-nowrap">Tiểu vùng quy hoạch</th>
                                        <th rowspan="2" class="text-center align-middle text-nowrap">Năm <br /> xây dựng</th>
                                        <th rowspan="2" class="text-center align-middle text-nowrap">Năm <br /> vận hành</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1])" colspan="30" class="text-center align-middle text-nowrap">Thông số của công trình</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[4])" colspan="19" class="text-center align-middle text-nowrap">Thông số của công trình</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[5])" colspan="22" class="text-center align-middle text-nowrap">Thông số của công trình</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[6])" colspan="7" class="text-center align-middle text-nowrap">Thông số của công trình</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[11])" colspan="3" class="text-center align-middle text-nowrap">Thông số của công trình</th>
                                        <th colspan="3" class="text-center align-middle text-nowrap">Thông tin cấp phép</th>
                                        <th colspan="2" class="text-center align-middle text-nowrap">Quyết định tiền cấp quyền</th>
                                        <th rowspan="2" class="text-center align-middle text-nowrap sticky-col end-col">Thao tác</th>
                                    </tr>
                                    <tr>
                                        <!-- lat,long -->
                                        <th class="text-center align-middle text-nowrap">X (m) </th>
                                        <th class="text-center align-middle text-nowrap">Y (m) </th>
                                        <!-- construction specifications -->
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">Cấp CT</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">F lưu vực <br />(km2) </th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">X <sub>TB năm</sub> <br />(m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">Q <sub>TB năm</sub><br />(m3/s)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5,6,11])" class="text-center align-middle text-nowrap">CS lắp máy (MW)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">CS <br /> đảm bảo(MW)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,5])" class="text-center align-middle text-nowrap">Chiều cao <br /> đập (m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,5])" class="text-center align-middle text-nowrap">Chiều dài <br />đập (m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,5])" class="text-center align-middle text-nowrap">Cao trình <br />đập (m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">Q<sub>max</sub>(m<sup>3</sup>/s)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">Q<sub>TT</sub>(m<sup>3</sup>/s)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">Q<sub>đảm bảo</sub>(m<sup>3</sup>/s)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">H<sub>max</sub> (m) </th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">H<sub>min</sub> (m) </th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">H<sub>TT</sub></th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">MNC(m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">MNDBT(m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">MNLTK(m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">MNLKT(m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">W<sub>toàn bộ</sub>(triệu m<sup>3</sup>)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">W<sub> chết </sub>(triệu m<sup>3</sup>)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" class="text-center align-middle text-nowrap">W<sub>hữu ích</sub>(triệu m<sup>3</sup>)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])" class="text-center align-middle text-nowrap">Số máy bơm</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,11])" class="text-center align-middle text-nowrap">Q<sub>TK</sub> (m<sup>3</sup>/h)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,11])" class="text-center align-middle text-nowrap">Q<sub>TT</sub> (m<sup>3</sup>/h)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])" class="text-center align-middle text-nowrap">F<sub>tưới TK</sub> (ha)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])" class="text-center align-middle text-nowrap">F<sub>tưới TT</sub> (ha)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])" class="text-center align-middle text-nowrap">T<sub>bơm TB</sub>(h)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])" class="text-center align-middle text-nowrap">T<sub>bơm min</sub>(h)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])" class="text-center align-middle text-nowrap">T<sub>bơm max</sub>(h)</th>
                                        <!--license-->
                                        <th class="text-center align-middle text-nowrap">Số GP</th>
                                        <th class="text-center align-middle text-nowrap">Ngày cấp</th>
                                        <th class="text-center align-middle text-nowrap">Thời hạn</th>
                                        <!--license fee-->
                                        <th rowspan="2" class="text-center align-middle text-nowrap">Số QĐ</th>
                                        <th rowspan="2" class="text-center align-middle text-nowrap">Tổng tiền (đồng) </th>
                                    </tr>
                                </thead>
                                <tbody class="custom-table-tbody">
                                    <tr ng-repeat="item in DataConstruction">
                                        <!-- lấy DataBieuMauSo1 từ file scrip/controler.js-->
                                        <!-- đưa (currentPage -1)*numPerPage+$index+1 để đếm hàng-->
                                        <td class="text-center align-middle sticky-col start-col"> {{(currentPage -1)*numPerPage+$index+1}} </td>
                                        <td class="text-nowrap sticky-col start-col" style="min-width:100px">
                                            <span ng-bind-html="item.ConstructionName" class="text-primary" ng-click="zoomConstruction(item.Lng, item.Lat, item.Id)"></span>
                                            <img class="grid-item-image" style="width: 15px;" src="~/LocalFiles/images/earth.png" alt="altImg">
                                        </td>
                                        <td style="min-width:200px" ng-bind-html="item.ConstructionLocation"></td>
                                        <td ng-bind-html="item.X"></td>
                                        <td ng-bind-html="item.Y"></td>
                                        <td style="min-width:200px" ng-bind-html="item.ExploitedWS"></td>
                                        <td style="min-width:200px" ng-bind-html="item.MiningMethod"></td>
                                        <td style="min-width:100px" ng-bind-html="item.MiningMode"></td>
                                        <td ng-bind-html="item.MiningPurpose"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" style="min-width:200px" ng-bind-html="item.BasinName"></td>
                                        <td style="width:100px" ng-bind-html="item.ConstructionTime"></td>
                                        <td style="width:100px" ng-bind-html="item.StartDate"></td>
                                        <!-- construction specifications -->
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.ConstructionLevel"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.BasinArea"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.RainAvgForYears"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.FlowAvgForYears"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5,6,11])" ng-bind-html="item.Power"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.GuaranteedPower"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,5])" ng-bind-html="item.DamHeight"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,5])" ng-bind-html="item.DamWidth"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,5])" ng-bind-html="item.DamElevation"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.MaximumFlow"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.MinimumFlow"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.GuaranteedFlow"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.Hmax"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.Hmin"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.Htt"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.DeadWL"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.RiseWL"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.DesignFloodLevel"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.CheckFloodWL"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.TotalCapacity"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.DeadCapacity"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.UsefulCapacity"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,6])" ng-bind-html="item.PumpNumber"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,11])" ng-bind-html="item.FlowDesigned"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,11])" ng-bind-html="item.RealityFlow"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,6])" ng-bind-html="item.WateringAreaDesigned"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,6])" ng-bind-html="item.RealityWateringArea"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,6])" ng-bind-html="item.AveragePumpTime"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,6])" ng-bind-html="item.MinimumPumpTime"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,6])" ng-bind-html="item.MaximumPumpTime"></td>
                                        <!--license-->
                                        <td>
                                            <div ng-repeat="license in item.Licenses">
                                                <span ng-click="openAsideFile(license)" class="text-primary text-nowrap" ng-bind-html="license.LicenseNumber"></span>
                                            </div>
                                        </td>
                                        <td>
                                            <div ng-repeat="license in item.Licenses">
                                                <span ng-bind-html="license.IssueDate | date:'MM/dd/yyyy'"></span>
                                            </div>
                                        </td>
                                        <td>
                                            <div ng-repeat="license in item.Licenses">
                                                <span ng-bind-html="license.Duration | date:'MM/dd/yyyy'"></span>
                                            </div>
                                        </td>
                                        <!--license fee-->
                                        <td>
                                            <div ng-repeat="licFee in item.LicenseFees">
                                                <span ng-click="openLicenseFeeFile(licFee)" class="text-primary text-nowrap" ng-bind-html="licFee.LicenseFeeNumber"></span>
                                            </div>
                                        </td>
                                        <td>
                                            <div ng-repeat="licFee in item.LicenseFees">
                                                <span ng-bind-html="licFee.TotalMoney | number : fractionSize"></span>
                                            </div>
                                        </td>
                                        <!--Action-->
                                        <td class="sticky-col end-col">
                                            <div class="d-flex">
                                                @if (_service.IsViewAction("/cong-trinh", User.Identity.Name, "EDIT"))
                                                {
                                                    <a href="javascript:void(0)" class="px-2" title="Chỉnh sửa công trình" ng-click="EditDataCons('formCreateCons' ,item)"><i class="fa fa-edit"></i></a>
                                                }
                                                @if (_service.IsViewAction("/cong-trinh", User.Identity.Name, "DELETE"))
                                                {
                                                    <a href="javascript:void(0)" class="px-2 text-danger" title="Xoá công trình" ng-click="DeleteItem(item)"><i class="fa-solid fa-trash-can"></i></a>
                                                }
                                            </div>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                            <!-- using for export -->
                            <table class="d-none" id="excel-export">
                                <thead>
                                    <tr>
                                        <th rowspan="2">STT</th>
                                        <th rowspan="2">Tên công trình</th>
                                        <th rowspan="2">Địa điểm</th>
                                        <th colspan="2">Tọa độ đập chính </th>
                                        <th rowspan="2">Nguồn nước KT</th>
                                        <th rowspan="2">Phương thức KT</th>
                                        <th rowspan="2">Chế độ KT</th>
                                        <th rowspan="2">Mục đích KT</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" rowspan="2">Tiểu vùng quy hoạch</th>
                                        <th rowspan="2">Năm xây dựng</th>
                                        <th rowspan="2">Năm vận hành</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1])" colspan="26">Thông số của công trình</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[4,5])" colspan="19">Thông số của công trình</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[6])" colspan="6">Thông số của công trình</th>
                                        <th colspan="3">Thông tin giấy phép</th>
                                        <th colspan="2">Tiền cấp quyền</th>
                                    </tr>
                                    <tr>
                                        <!-- lat,long -->
                                        <th>X (m) </th>
                                        <th>Y (m) </th>
                                        <!-- construction specifications -->
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Cấp công trình</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Diện tích lưu vực</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Lượng mưa trung bình nhiều năm</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Lưu lượng trung bình nhiều năm</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5,6])">Công suất (MW)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Công suất đảm bảo (MW)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Chiều cao đập (m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Qmax qua NM(m3/s)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Qtt(m3/s)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Q đảm bảo(m3/s)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Hmax</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Hmin</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">Htt</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">MNC(m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">MNDBT(m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">MNLTK(m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">MNLKT(m)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">W toàn bộ (triệu m3)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">W chết (triệu m3)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])">W hữu ích (triệu m3)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])">Số máy bơm</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])">Lưu lượng thiết kế (m3/h)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])">Lưu lượng thực tế (m3/h)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])">Diện tích tưới thiết kế (ha)</th>
                                        <th ng-if="ShowColumns(TypeOfConstructionId,[1,6])">Diện tích tưới thực tế (ha)</th>
                                        <!--license-->
                                        <th>Số GP</th>
                                        <th>Ngày cấp</th>
                                        <th>Thời hạn</th>
                                        <!--license fee-->
                                        <th rowspan="2">QĐ cấp quyền</th>
                                        <th rowspan="2">Tổng tiền cấp quyền (đồng) </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="item in DataConsForExportExcel">
                                        <!-- lấy DataBieuMauSo1 từ file scrip/controler.js-->
                                        <!-- đưa (currentPage -1)*numPerPage+$index+1 để đếm hàng-->
                                        <td> {{(currentPage -1)*numPerPage+$index+1}} </td>
                                        <td ng-bind-html="item.ConstructionName"></td>
                                        <td ng-bind-html="item.ConstructionLocation"></td>
                                        <td ng-bind-html="item.X"></td>
                                        <td ng-bind-html="item.Y"></td>
                                        <td ng-bind-html="item.ExploitedWS"></td>
                                        <td ng-bind-html="item.MiningMethod"></td>
                                        <td ng-bind-html="item.MiningMode"></td>
                                        <td ng-if="ShowColumns(TypeOfConstructionId,[1,4,5])" ng-bind-html="item.SmallPlanningArea"></td>
                                        <td ng-bind-html="item.ConstructionTime"></td>
                                        <td ng-bind-html="item.StartDate"></td>
                                        <!-- construction specifications -->
